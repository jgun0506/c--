#include <stdio.h>

void air(char str[1000]){
	for(int i=0;i<sizeof(str);i++){
		if(str[i]=" "){
			str[i]=str[i+1];
		}
	}
}

void capitalize_first_character(char str[1000]) {
    int i;
	for(i=0;str[i];i++)
		;
	int len=i;
    
    for (int i = 0; i < len; i++) {
        if (str[i] == ' ' && str[i+1] != ' ') {
            str[i + 1] -= 32;
        }
    }
    
}
int main(void) {
    char str[1000];

    fgets(str, sizeof(str), stdin);

    capitalize_first_character(str);
    printf("%s", str);
    return 0;
}
